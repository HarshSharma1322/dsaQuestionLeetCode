void findPreSuc(Node* root, Node*& pre, Node*& suc, int key)
{
//SUCCESSOR
    Node *curr=root;
    while(curr){
        if(curr->key>key){
            suc=curr;
            curr=curr->left;
        }
        else{
            curr=curr->right;
        }
        
   // Predecessor
        while(root){
            if(root->key>=key){
                root=root->left;
            }
            else{
                pre=root;
                root=root->right;
            }
        }
    }

}
